<h1>Listing campanhas_agentes</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Inicio</th>
    <th>Termino</th>
    <th>Regulamento</th>
    <th>Pontos venda</th>
    <th>Pontos vendedor</th>
  </tr>

<% @campanhas_agentes.each do |campanhas_agente| %>
  <tr>
    <td><%=h campanhas_agente.nome %></td>
    <td><%=h campanhas_agente.inicio %></td>
    <td><%=h campanhas_agente.termino %></td>
    <td><%=h campanhas_agente.regulamento %></td>
    <td><%=h campanhas_agente.pontos_venda %></td>
    <td><%=h campanhas_agente.pontos_vendedor %></td>
    <td><%= link_to 'Show', campanhas_agente %></td>
    <td><%= link_to 'Edit', edit_campanhas_agente_path(campanhas_agente) %></td>
    <td><%= link_to 'Destroy', campanhas_agente, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

