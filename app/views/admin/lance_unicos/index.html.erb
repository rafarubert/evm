<h1>Listing lance_unico</h1>

<table>
  <tr>
    <th>Premios</th>
    <th>Quantidade</th>
  </tr>

<% @lance_unicos.each do |lance_unico| %>
  <tr>
    <td><%=h lance_unico.premios %></td>
    <td><%=h lance_unico.quantidade %></td>
    <td><%= link_to 'Edit', edit_admin_lance_unico_path(lance_unico) %></td>
    <td><%= link_to 'Destroy', [:admin, lance_unico], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<% form_for([:admin, @lance_unico]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :premios %><br />
    <%= f.text_field :premios %>
  </p>
  <p>
    <%= f.label :data_inicio %><br />
    <%= f.date_select :data_inicio %>
  </p>
  <p>
    <%= f.label :data_termino %><br />
    <%= f.date_select :data_termino %>
  </p>
  <p>
    <%= f.label :quantidade %><br />
    <%= f.text_field :quantidade %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
<%= link_to 'New lance_unico', new_admin_lance_unico_path %>